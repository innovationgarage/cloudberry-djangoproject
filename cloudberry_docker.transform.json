{
    "containers": [
        {
            "$": "$.context.fk['cloudberry_app.Config'][$.context.referred_in_configs.*.id]",
            "uuid": {
                "$": "@template().config.server.device.id"
            },
            "key": {
                "$": "$.context.fk['cloudberry_app.Device'][@template().config.server.device.id].key"
            },
            "ports": [
                {
                    "proto": "tcp",
                    "guest": 1194,
                    "host": {
                        "$": "@template().server.port"
                    }
                }
            ]
        }
    ]
}
